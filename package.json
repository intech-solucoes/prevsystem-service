{
	"name": "@intechprev/prevsystem-service",
	"version": "2.0.0",
	"main": "dist/index.js",
	"typings": "./types/index.d.ts",
	"dependencies": {
		"@intechprev/service": "file:../lib/service"
	},
	"devDependencies": {
		"@types/node": "10.12.18",
		"rimraf": "^2.6.3",
		"typescript": "3.2.4"
	},
	"scripts": {
		"build-types": "yarn clean && tsc -d --declarationDir ./types",
		"build": "yarn && yarn build-types && node ./node_modules/rimraf/bin.js ./node_modules",
		"clean": "node ./node_modules/rimraf/bin.js ./types && node ./node_modules/rimraf/bin.js ./dist",
		"npm:publish": "yarn build && npm publish --access public"
	}
}